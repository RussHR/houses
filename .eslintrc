{
	"extends": [
		"airbnb",
		"plugin:react/recommended"
	],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},
	"rules": {
		"indent": ["error", "tab"],
		"no-tabs": 0
	},
	"settings": {
		"react": {
			"version": "16.8"
		}
	}
}
